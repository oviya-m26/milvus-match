    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "globby": "^13.2.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.21.1",
    "papaparse": "^5.4.1",
    "string-similarity": "^4.0.4",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
